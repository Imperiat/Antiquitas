# OPEN WITH: gui.createwidget religion_chinese.gui religion_chinese

religion_window = {
	name = "religion_chinese" #name used in console command to open widget
	blockoverride "Title_text" {
		text = "One Hundred Schools of Thought"
	}
	blockoverride "On_close" {
		onclick = "[ExecuteConsoleCommand('GUI.ClearWidgets religion_chinese')]"
	}
	blockoverride "button_one" {
		texture = "gfx/interface/religion_icons/chinese_leader.dds"
		modify_texture = {
			texture = "gfx/interface/base_parts/dark_bg_square_texture.dds"
			blend_mode = Multiply
		}
	}
	blockoverride "button_two" {
		texture = "gfx/interface/religion_icons/chinese_ruler.dds"
		modify_texture = {
			texture = "gfx/interface/base_parts/dark_bg_square_texture.dds"
			blend_mode = Multiply
		}
	}
	blockoverride "button_three" {
		texture = "gfx/interface/religion_icons/chinese_building.dds"
		modify_texture = {
			texture = "gfx/interface/base_parts/dark_bg_square_texture.dds"
			blend_mode = Multiply
		}
	}
	blockoverride "button_four" {
		texture = "gfx/interface/religion_icons/chinese_siege.dds"
		modify_texture = {
			texture = "gfx/interface/base_parts/dark_bg_square_texture.dds"
			blend_mode = Multiply
		}
	}
}