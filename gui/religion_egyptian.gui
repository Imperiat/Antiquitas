# OPEN WITH: gui.createwidget religion_egyptian.gui religion_egyptian

religion_window = {
	name = "religion_egyptian" #name used in console command to open widget
	blockoverride "Title_text" {
		text = "The Ways of Ma'at"
	}
	blockoverride "On_close" {
		onclick = "[ExecuteConsoleCommand('GUI.ClearWidgets religion_egyptian')]"
	}
	blockoverride "button_one" {
		texture = "gfx/interface/religion_icons/egypt_river.dds"
		modify_texture = {
			texture = "gfx/interface/base_parts/dark_bg_square_texture.dds"
			blend_mode = Multiply
		}
	}
	blockoverride "button_two" {
		texture = "gfx/interface/religion_icons/egypt_pyramids.dds"
		modify_texture = {
			texture = "gfx/interface/base_parts/dark_bg_square_texture.dds"
			blend_mode = Multiply
		}
	}
	blockoverride "button_three" {
		texture = "gfx/interface/religion_icons/egypt_treasure.dds"
		modify_texture = {
			texture = "gfx/interface/base_parts/dark_bg_square_texture.dds"
			blend_mode = Multiply
		}
	}
	blockoverride "button_four" {
		texture = "gfx/interface/religion_icons/egypt_desert.dds"
		modify_texture = {
			texture = "gfx/interface/base_parts/dark_bg_square_texture.dds"
			blend_mode = Multiply
		}
	}
}